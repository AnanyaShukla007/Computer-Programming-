#include <stdio.h>
int main()
{
int n1, n2, n3, n4, n5;
float total, avg, per;
printf("Enter number for subject 1: ");
scanf("%d",n1);
printf("Enter number for subject 2: "); 
scanf("%d",n2);
printf("Enter number for subject 3: "); 
scanf("%d",n3);
printf("Enter number for subject 4: "); 
scanf("%d",n4);
printf("Enter number for subject 5: "); 
scanf("%d",n5);
total= n1+n2+n3+n4+n5; 
avg = total/5;
per= (total/500)*100; 
printf("Total=",total); 
printf("Average=",avg);
printf("Percentage=",per); 
return 0;
}
